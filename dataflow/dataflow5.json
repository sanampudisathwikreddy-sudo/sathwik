{
	"name": "dataflow5",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText38",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable37",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable40",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable41",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "split1"
				},
				{
					"name": "AlterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderID as integer,",
				"          OrderDate as date,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as integer,",
				"          Price as integer,",
				"          TotalAmount as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          OrderID as integer,",
				"          OrderDate as date,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as integer,",
				"          Price as integer,",
				"          TotalAmount as integer,",
				"          active_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 derive(hashkey_1 = md5(OrderDate,Product,Quantity,Price,TotalAmount),",
				"          md = currentTimestamp()) ~> derivedColumn1",
				"derivedColumn1, derivedColumn2 lookup(source1@OrderID == source2@OrderID,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"source2 derive(hash_key2 = md5(CustomerName,Product,Quantity,Price,TotalAmount)) ~> derivedColumn2",
				"lookup1 split(isNull(source2@OrderID),",
				"     !isNull(source2@OrderID)&&notEquals(hashkey_1, hash_key2),",
				"     disjoint: false) ~> split1@(insert, update)",
				"split1@update alterRow(updateIf(1==1)) ~> AlterRow1",
				"split1@insert sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          OrderID as integer,",
				"          OrderDate as date,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as integer,",
				"          Price as integer,",
				"          TotalAmount as integer,",
				"          modified_date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          OrderID = split1@insert@OrderID,",
				"          OrderDate = split1@insert@OrderDate,",
				"          CustomerName = split1@insert@CustomerName,",
				"          Product = split1@insert@Product,",
				"          Quantity = split1@insert@Quantity,",
				"          Price = split1@insert@Price,",
				"          TotalAmount = split1@insert@TotalAmount,",
				"          modified_date = md",
				"     )) ~> sink1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          OrderID as integer,",
				"          OrderDate as date,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as integer,",
				"          Price as integer,",
				"          TotalAmount as integer,",
				"          modified_date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          OrderID = split1@update@OrderID,",
				"          OrderDate = split1@update@OrderDate,",
				"          CustomerName = split1@update@CustomerName,",
				"          Product = split1@update@Product,",
				"          Quantity = split1@update@Quantity,",
				"          Price = split1@update@Price,",
				"          TotalAmount = split1@update@TotalAmount,",
				"          modified_date = md",
				"     )) ~> sink2"
			]
		}
	}
}