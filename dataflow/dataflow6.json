{
	"name": "dataflow6",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText39",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable42",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderID as string,",
				"          OrderDate as string,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as string,",
				"          Price as string,",
				"          TotalAmount as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          OrderID as integer,",
				"          OrderDate as date,",
				"          CustomerName as string,",
				"          Product as string,",
				"          Quantity as integer,",
				"          Price as integer,",
				"          TotalAmount as integer,",
				"          modified_date as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 derive(hash_key1 = md5(CustomerName,Product,Quantity,Price,TotalAmount),",
				"          status1 = 1) ~> derivedColumn1",
				"source2 derive(hash_key2 = md5(CustomerName,Product,Quantity,Price,TotalAmount),",
				"          status0 = 0) ~> derivedColumn2"
			]
		}
	}
}