{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText35",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable27",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable28",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "exists1"
				},
				{
					"name": "AlterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          student_id as integer,",
				"          student_name as string,",
				"          course as string,",
				"          year as integer,",
				"          marks as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          student_id as integer,",
				"          student_name as string,",
				"          course as string,",
				"          year as integer,",
				"          marks as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 derive(hash_key1 = md5(student_name,course,year,marks)) ~> derivedColumn1",
				"source2 derive(hash_key2 = md5(student_name,course,year,marks)) ~> derivedColumn2",
				"derivedColumn1, derivedColumn2 exists(source1@student_id == source2@student_id",
				"     && hash_key1 == hash_key2,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"exists1 alterRow(upsertIf(1==1)) ~> AlterRow1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          student_id as integer,",
				"          student_name as string,",
				"          course as string,",
				"          year as integer,",
				"          marks as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['student_id'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          student_id,",
				"          student_name,",
				"          course,",
				"          year,",
				"          marks",
				"     )) ~> sink1"
			]
		}
	}
}